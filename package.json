{
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/better-npm-run build",
    "prod": "./node_modules/.bin/better-npm-run prod",
    "start": "./node_modules/.bin/better-npm-run build",
    "serve": "./node_modules/.bin/better-npm-run serve",
    "laravel": "./node_modules/.bin/better-npm-run laravel",
    "laravel:prod": "./node_modules/.bin/better-npm-run laravel:prod",
    "sockets": "node server/index.js",
    "sockets:prod": "node server/index.js"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "serve": {
      "command": "webpack-dev-server --config webpack/webpack.dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "laravel": {
      "command": "webpack-dev-server --config webpack/webpack.laravel.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "laravel:prod": {
      "command": "webpack-dev-server --config webpack/webpack.laravel.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "sockets": {
      "command": "nodemon server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "sockets:prod": {
      "command": "nodemon server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.14",
    "clean-css": "^4.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.4.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "socket.io-client": "^1.7.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel": "^6.5.2",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "ioredis": "^2.5.0",
    "nodemon": "^1.11.0",
    "nprogress": "^0.2.0",
    "promise.prototype.finally": "^2.0.1",
    "react": "^15.4.2",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-formal": "^0.24.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "style-loader": "^0.13.1",
    "yup": "^0.21.3"
  }
}
